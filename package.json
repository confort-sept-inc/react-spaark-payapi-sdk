{
  "name": "spaark-payapi-sdk",
  "version": "1.0.0",
  "description": "TypeScript SDK for Pawapay Mobile Money API (V2) - Deposits, Payouts, Refunds for Africa",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./react": {
      "types": "./dist/react.d.ts",
      "import": "./dist/react.mjs",
      "require": "./dist/react.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts,.tsx",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "axios": "^1.6.0",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "dependencies": {
    "uuid": "^9.0.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/uuid": "^9.0.0",
    "axios": "^1.6.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "react": "^18.0.0",
    "ts-jest": "^29.1.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  },
  "keywords": [
    "pawapay",
    "mobile-money",
    "momo",
    "mtn",
    "orange-money",
    "airtel",
    "payments",
    "africa",
    "cameroon",
    "congo",
    "gabon",
    "cemac",
    "xaf",
    "sdk",
    "typescript",
    "fintech",
    "spaark"
  ],
  "author": "Spaark <contact@spaark.cloud>",
  "license": "MIT",
  "homepage": "https://github.com/spaark-cloud/spaark-payapi-sdk#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spaark-cloud/spaark-payapi-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/spaark-cloud/spaark-payapi-sdk/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
